   <children>
      <ComboBox fx:id="combobox" layoutX="7.0" layoutY="56.0" onAction="#comboChanged" prefHeight="48.0" prefWidth="200.0" />
      <Label fx:id="mylabel" layoutX="14.0" layoutY="14.0" prefHeight="48.0" prefWidth="170.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="223.0" layoutY="60.0" mnemonicParsing="false" onAction="#addItems" prefHeight="40.0" prefWidth="77.0" text="Add Items" />
      <ListView fx:id="listview" layoutX="14.0" layoutY="104.0" prefHeight="181.0" prefWidth="279.0" />
   </children>
</AnchorPane>
